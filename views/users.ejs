<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Teste Front</title>
  </head>

  <body>
    <main></main>
  </body>
  <script>
    async function getContent() {
      try {
        const response = await fetch("COLOCAR ENDEREÇO AQUI");
        const data = await response.json();
        console.log(data);

        show(data);
      } catch (error) {
        console.log(error);
      }
    }

    getContent();

    // funcão para jogar os dados na tela
    function show(users) {
      let output = "";

      for (let user of users) {
        output += `<li>${user.fullName}</li>`;
      }
      document.querySelector("main").innerHTML = output;
    }
  </script>
</html>
